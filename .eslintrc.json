{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true,
    "mocha": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:mocha/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "mocha"
  ],
  "rules": {
    "mocha/no-mocha-arrows":"off"
  }
}
